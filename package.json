{
  "name": "less-hashed",
  "version": "1.0.1a",
  "description": "A utility to hashed less files, make tree of all less files and his dependencies with logs and return only changed files",
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha --require babel-polyfill --compilers js:babel-register",
    "build": "babel lib --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build",
    "test-travis": "make test-travis"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ivandata/less-hashed.git"
  },
  "keywords": [
    "hash",
    "less",
    "dependencies",
    "@import",
    "tree"
  ],
  "author": "Ivan Malov <nuclearfunk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivandata/less-hashed/issues"
  },
  "homepage": "https://github.com/ivandata/less-hashed#readme",
  "dependencies": {
    "chai": "^3.5.0",
    "chai-fs": "^1.0.0",
    "colors": "^1.1.2",
    "fake-fs": "^0.5.0",
    "fs": "0.0.1-security",
    "glob": "^7.1.1",
    "hash-files": "^1.1.1",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1"
  }
}
